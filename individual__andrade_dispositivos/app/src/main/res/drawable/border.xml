<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#e85d04"
                android:centerColor="#dc2f02"
                android:endColor="#e85d04"
                android:angle="45"/>
            <corners android:radius="8dp"/>
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp"/>
        </shape>
    </item>
</layer-list>
